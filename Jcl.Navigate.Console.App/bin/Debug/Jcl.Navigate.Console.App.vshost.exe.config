<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <system.serviceModel>
    <client>
      <endpoint address="http://cloud.tfl.gov.uk"
              binding="webHttpBinding"
              contract="Jcl.Navigate.Services.Contracts.ILineStatus"
              behaviorConfiguration="WebHttpBehaviour" />
    </client>
    <behaviors>
      <endpointBehaviors>
        <behavior name="WebHttpBehaviour">
          <webHttp />
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>

</configuration>